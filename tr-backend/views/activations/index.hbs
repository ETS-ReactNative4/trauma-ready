<h1>{{viewTitle}}</h1>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Activation Id</th>
        <th>Activation Name</th>
        <th>Age</th>
        <th>Regional Advisory Committee</th>
        <th>Trauma</th>
        <th>Notes</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each list}}
        <tr>
            <td>{{this.aid}}</td>
            <td>{{this.name}}</td>
            <td>{{this.age}}</td>
            <td>{{this.rac}}</td>
            <td>
              <div class="ul-container">
                <ul>
                  {{#each this.trauma}}
                    <li>{{this}}</li>
                  {{/each}}
                </ul>
              </div>
            </td>

            <td>{{this.notes}}</td>
            <td></td>
        </tr>
    {{/each}}
    </tbody>
</table>
